pipeline {
    agent any
    
    stages{
        stage('build'){
            steps{
                bat "mvn clean compile"
            }
        }
        stage('test'){
            steps{
                bat "mvn test"
            }
        }
        stage('package'){
        	steps{
                bat "mvn -DskipTests package"
            }
        }
        stage('deliver'){
        	steps{
        		bat "./scripts/deliver.bat"
        	}
        }
    }
}